{
  "name": "macOS 14 (Sonoma)",
  "service": "macos",
  "containerEnv": {
    "VERSION": "14",
    "RAM_SIZE": "24G",
    "CPU_CORES": "6"
  },
  "forwardPorts": [8006],
  "portsAttributes": {
    "8006": {
      "label": "Web",
      "onAutoForward": "notify"
    }
  },
  "otherPortsAttributes": {
    "onAutoForward": "ignore"
  }, 
  "dockerComposeFile": "codespaces.yml",
  "workspaceFolder": "/workspaces/macos",
  "initializeCommand": "docker system prune --all --force"
}
